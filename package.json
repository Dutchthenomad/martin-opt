{
  "name": "martingale-optimizer",
  "version": "5.3.0",
  "description": "Real-time decision support system for cryptocurrency side betting with advanced probability calculations and martingale strategy optimization",
  "main": "src/server/data-save-server.js",
  "scripts": {
    "start": "npm run server",
    "server": "node src/server/data-save-server.js",
    "dashboard": "echo 'Open src/ui/mobile-dashboard.html in your browser'",
    "analyze": "node src/analysis/recommendation_analyzer.js",
    "test": "npm run test:unit && npm run test:integration",
    "test:unit": "node tests/unit/zone_framework.test.js && node tests/unit/enhanced_martingale.test.js",
    "test:integration": "node tests/integration/integrated_system.test.js",
    "test:patterns": "node tests/unit/hidden_patterns.test.js",
    "clean": "rm -rf data/live/*.json data/archive/*.json",
    "dev": "npm run server"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "express": "^5.1.0",
    "glob": "^10.3.10",
    "socket.io-client": "^4.7.5"
  },
  "keywords": [
    "rugs.fun",
    "side-bet",
    "martingale",
    "trading",
    "cryptocurrency",
    "solana",
    "probability",
    "risk-management",
    "real-time",
    "websocket"
  ],
  "author": "Martingale Optimizer Team",
  "license": "MIT",
  "engines": {
    "node": ">=14.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/martingale-optimizer-v5.3.git"
  },
  "bugs": {
    "url": "https://github.com/yourusername/martingale-optimizer-v5.3/issues"
  }
}
